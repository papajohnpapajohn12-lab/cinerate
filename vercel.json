{
  "version": 2,
  "outputDirectory": ".",
  "rewrites": [
    {"source": "/api/(.*)", "destination": "/api/index.py"}
  ],
  "headers": [
    {
      "source": "/frontend/(.*)",
      "headers": [
        {"key": "Cache-Control", "value": "public, max-age=31536000, immutable"}
      ]
    }
  ]
}
